<?xml version="1.0" encoding="utf-8"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-ProposalsFuncao">
	<DisplayName>AM-ProposalsFuncao</DisplayName>
	<FaultRules></FaultRules>
	<Properties></Properties>
	<Set>
		<Headers>
			<Header name="Authorization">Bearer {bffToken.access_token}</Header>
			<Header name="Content-Type">application/json</Header>
		</Headers>
		<Payload contentType="application/json">{
            	"Pesquisa": {
            		"CPF": "{cpfFormat}"
            	},
            	"Ordenacao": {
            		"CampoOrdenacao": "{payload.order_by}",
            		"TipoOrdenacao": "{payload.order}"
            	},
            	"Paginacao": {
            		"Quantidade": {payload.quantity},
            		"NumeroPagina": {payload.page}
            	},
            	"Filtros": {
            		"TipoConsulta": "{payload.status}"
            	}
            }</Payload>
		<Verb>POST</Verb>
		<Path>{path.c6consig-bff-funcao}/FIApiAutEsteira/Esteira/ConsultaPropostaEsteira</Path>
	</Set>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	<AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>