<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-GetTagsByCpf">
    <DisplayName>AM-GetTagsByCpf</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Content-Type">application/json</Header>
            <Header name="Authorization">{private.Authorization.cartag.service}</Header>
        </Headers>
        <Payload contentType="application/json">
            {
             "cpf": "{cpf}"
            }
        </Payload>
        <Verb>POST</Verb>
        <Path>/person/tags</Path>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="true">request</AssignTo>
</AssignMessage>
