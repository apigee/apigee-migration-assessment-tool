<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="true" enabled="true" name="AM-Claim">
    <DisplayName>AM-Claim</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Authorization">{private.Authorization.claim.service}</Header>
        </Headers>
        <Path>/claims{proxy.pathsuffix}</Path>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>
