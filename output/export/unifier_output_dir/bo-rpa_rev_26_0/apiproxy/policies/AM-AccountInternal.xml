<?xml version="1.0" encoding="utf-8"?>
<AssignMessage async="false" continueOnError="true" enabled="true" name="AM-AccountInternal">
	<DisplayName>AM-AccountInternal</DisplayName>
	<FaultRules></FaultRules>
	<Properties></Properties>
	<Set>
		<Headers>
			<Header name="Authorization">{private.Authorization.cards.manager}</Header>
			<Header name="Accept">application/vnd.c6bank+json; version=2</Header>
		</Headers>
		<Verb>GET</Verb>
		<Path>/internal/account/{payload.account_id}</Path>
	</Set>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	<AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>