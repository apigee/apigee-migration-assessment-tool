<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description></Description>
    <FaultRules/>
    <Flows>
        <Flow name="newman-george">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/newman-george")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>mccann-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>ponce-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>jenkins-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>hampton-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>powers-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>watson-liu-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>edwards-wilkinson-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>smith-owens-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>hays-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>moore-farmer-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>spencer-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>golden-carter-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>larson-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>martin-townsend-callout</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/newman-george</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
