<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>SEC-VerifyAPIKey</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>MED-AM-SecretKey</Name>
            </Step>
            <Step>
                <Name>SEC-GenerateJWT</Name>
            </Step>
            <Step>
                <Name>MED-AM-JWTResponseHeader</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPProxyConnection>
        <BasePath>/aks-bom-login</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>