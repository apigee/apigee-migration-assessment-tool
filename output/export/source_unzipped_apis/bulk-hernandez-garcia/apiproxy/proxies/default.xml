<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description></Description>
    <FaultRules/>
    <Flows>
        <Flow name="hernandez-garcia">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/hernandez-garcia")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>howard-dunlap-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>perez-hartman-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>martin-shaw-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>hill-gallagher-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>johnston-callout</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/hernandez-garcia</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
