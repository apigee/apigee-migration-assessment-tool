<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description></Description>
    <FaultRules/>
    <Flows>
        <Flow name="mays-stewart">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/mays-stewart")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>jenkins-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>walker-king-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>howard-dunlap-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>johnston-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>medina-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>lowery-johnson-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>odom-hampton-callout</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/mays-stewart</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
