<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description></Description>
    <FaultRules/>
    <Flows>
        <Flow name="may">
            <Description></Description>
            <Request/>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/may")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>jenkins-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>mccann-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>thompson-johnson-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>long-martinez-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>morris-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>ward-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>warner-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>smith-owens-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>simon-brown-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>morgan-morgan-callout</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>larson-callout</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/may</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
