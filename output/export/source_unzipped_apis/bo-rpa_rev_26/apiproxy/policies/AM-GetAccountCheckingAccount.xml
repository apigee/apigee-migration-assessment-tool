<?xml version="1.0" encoding="utf-8"?>
<AssignMessage async="false" continueOnError="true" enabled="true" name="AM-GetAccountCheckingAccount">
	<DisplayName>AM-GetAccountCheckingAccount</DisplayName>
	<FaultRules></FaultRules>
	<Properties></Properties>
	<Set>
		<Headers>
			<Header name="Authorization">{private.Authorization.checking.account}</Header>
			<Header name="Accept">application/vnd.c6bank_branches_accounts_v2+json</Header>
		</Headers>
		<Verb>GET</Verb>
		<Path>/branches/{payload.branch_number}/accounts/{payload.account_number}</Path>
	</Set>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	<AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>