<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-GetProductsCredit">
    <DisplayName>AM-GetProductsCredit</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Authorization">{private.Authorization.credit.manager}</Header>
            <Header name="Accept">application/vnd.creditlimit+json; version=2</Header>
        </Headers>
        <QueryParams>
            <QueryParam name="reloadProducts">true</QueryParam>
        </QueryParams>
        <Verb>GET</Verb>
        <Path>/credit-limits/{payload.person_id}</Path>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>
