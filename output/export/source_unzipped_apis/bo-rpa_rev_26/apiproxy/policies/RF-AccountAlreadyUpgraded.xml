<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-AccountAlreadyUpgraded">
    <DisplayName>RF-AccountAlreadyUpgraded</DisplayName>
    <FaultRules/>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json">
                {
                    "type": "ACCOUNT_ALREADY_UPGRADED", 
                    "message": "Account already upgraded"
                }
            </Payload>
            <StatusCode>406</StatusCode>
            <ReasonPhrase>Not Acceptable</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <ShortFaultReason>false</ShortFaultReason>
</RaiseFault>
