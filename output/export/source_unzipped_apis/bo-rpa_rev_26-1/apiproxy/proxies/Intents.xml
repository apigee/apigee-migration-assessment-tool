<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="Intents">
    <Description/>
    <FaultRules/>
    <Flows>
        <Flow name="GetIntents">
            <Request>
                <Step>
                    <FaultRules/>
                    <Name>KV-LoadUraServiceCredentials</Name>
                </Step>
                <Step>
                    <FaultRules/>
                    <Name>AM-GetIntents</Name>
                </Step>
            </Request>
            <Response/>
            <Condition>(proxy.pathsuffix MatchesPath "/") and (request.verb = "GET")</Condition>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>OA-VerifyAccessToken</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/bo-rpa/intents</BasePath>
        <Properties/>
        <VirtualHost>vhost-backoffice</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="UraService">
        <TargetEndpoint>UraService</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>