<?xml version="1.0" encoding="utf-8"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-CheckingAccountReactivation">
	<DisplayName>AM-CheckingAccountReactivation</DisplayName>
	<FaultRules></FaultRules>
	<Properties></Properties>
	<Set>
		<Headers>
			<Header name="Authorization">{private.Authorization.checking.manager}</Header>
		</Headers>
		<Payload contentType="application/json">{
                "person_id": "{person.person_id}",
                "request_by": "{reactiveCheckingAccountPayload.request_by}"
            }</Payload>
		<Verb>POST</Verb>
		<Path>/person/account/reactive</Path>
	</Set>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	<AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>