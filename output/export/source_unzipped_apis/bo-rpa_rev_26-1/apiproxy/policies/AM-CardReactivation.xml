<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-CardReactivation">
    <DisplayName>AM-CardReactivation</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Authorization">{private.Authorization.cards.manager}</Header>
            <Header name="Accept">application/vnd.c6bank+json</Header>
        </Headers>
        <Verb>POST</Verb>
        <Path>/account/{account.id}/reactivate</Path>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false">request</AssignTo>
</AssignMessage>
